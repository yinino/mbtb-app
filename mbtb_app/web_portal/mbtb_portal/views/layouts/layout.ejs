            </ul>

            <% // check for auth token if user is logged in or not %>
            <% if (req.session.auth_token === undefined) { %>
              <ul class="nav navbar-nav navbar-right">
                <li><a href="/register"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
                <li><a href="/login"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
              </ul>
            <% } else if(req.session.auth_token !== undefined && req.session.auth_token.length > 0){%>
              <ul class="nav navbar-nav navbar-right">
                <li><a href="/logout" id="user_logout"><span class="glyphicon glyphicon-log-out"></span> Logout</a></li>
              </ul>
            <% }%>

